<?xml version="1.0" encoding="utf-8"?>
<package
	name="wingstall"
	appName="Wingstall"
	version="1.1.0"
	publisher="Seppo Laakko"
	sourceRootDir="../.."
	targetRootDir="C:/wingstall"
	compression="bzip2"
	iconFilePath="$TARGET_ROOT_DIR$/icon/setup.ico"
	includeUninstaller="true"
	id="17e24c2e-5ddf-4b18-b4ee-1d66182ab193">
	<component
		name="root">
		<file 
			name="wingstall.sln"/>
	</component>
	<component
		name="binaries">
		<directory
			name="bin">
		</directory>
	</component>
	<component
		name="libraries">
		<directory
			name="lib">
		</directory>
	</component>
	<component
		name="unicode">
		<directory
			name="unicode">
			<exclude
				dir="x64"/>
		</directory>
	</component>
	<component
		name="configuration">
		<directory
			name="config">
		</directory>
	</component>
	<component
		name="documentation">
		<directory
			name="doc">
		</directory>
	</component>
	<component
		name="ext">
		<directory
			name="ext">
			<exclude
				dir="Debug" cascade="true"/>
			<exclude
				dir="Release" cascade="true"/>
			<exclude
				dir="x64" cascade="true"/>
		</directory>
	</component>
	<component
		name="source">
		<directory
			name="filecompare">
			<exclude
				dir="x64"/>
		</directory>
		<directory
			name="make_package_id">
			<exclude
				dir="x64"/>
		</directory>
		<directory
			name="sngxml">
			<exclude 
				dir="x64" cascade="true"/>
		</directory>
		<directory
			name="soulng">
			<exclude
				dir="x64" cascade="true"/>
		</directory>
		<directory
			name="uninstall">
			<exclude
				dir="x64"/>
		</directory>
		<directory	
			name="wing">
			<exclude
				dir="x64"/>
		</directory>
		<directory
			name="winggui">
			<exclude
				dir="x64"/>
		</directory>
		<directory
			name="wingpackage">
			<exclude
				dir="x64"/>
		</directory>
		<directory
			name="wingstall">
			<exclude
				dir="x64"/>
		</directory>
		<directory
			name="wnginst">
			<exclude
				dir="x64"/>
		</directory>
	</component>
	<component
		name="icon">
		<directory
			name="icon"/>
	</component>
	<component
		name="install">
		<directory
			name="inst"/>
	</component>
	<component
		name="setup">
		<directory
			name="setup">
			<exclude
				dir="bin" cascade="true"/>
			<exclude
				dir="program" cascade="true"/>
			<exclude
				file="*.bin" cascade="true"/>
			<exclude
				file="*.index.xml" cascade="true"/>
			<exclude
				file="*.info.xml" cascade="true"/>
		</directory>
	</component>
	<component
		name="example">
		<directory
			name="example">
			<include 
				dir="setup">
				<exclude
					dir="bin" cascade="true"/>
				<exclude
					dir="program" cascade="true"/>
				<exclude 
					file="*.bin" cascade="true"/>
				<exclude
					file="*.index.xml" cascade="true"/>
				<exclude
					file="*.info.xml" cascade="true"/>
			</include>
			<include
				dir="source">
				<include 
					dir="example">
					<exclude dir=".vs" cascade="true"/>
					<exclude dir="x64" cascade="true"/>
					<exclude dir="Debug" cascade="true"/>
					<exclude dir="Release" cascade="true"/>
				</include>
			</include>
		</directory>
	</component>
	<component
		name="resources">
		<directory
			name="res">
		</directory>
	</component>
	<environment>
		<variable name="WINGSTALL_ROOT" value="$TARGET_ROOT_DIR$"/>
		<pathDirectory value="$TARGET_ROOT_DIR$/bin"/>
	</environment>
</package>
